<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<!-- DO NOT EDIT THIS FILE. CUSTOM CHANGES MUST BE DONE IN /opt/site24x7/monagent/conf/custom_monitors.xml-->
<!-- 
Attributes :
	script		=	"script=true" tells the agent to append the script location before the "command=cpu.sh" attribute
	sendValue	=	"sendValue=false" implies attribute value parsed from command output will not be sent to server
 -->
<MonitorsXml>
	<LinuxMonitors>
		<Monitor Id="CPU Utilization" script="true" command="script.sh" commandArgs="cpu_util,processor" parseTag="cpu details,processor" interval="30" parseImpl="ROOT_IMPL,ASSET_IMPL">
			<Entity Id="DeviceID" parseLine="1" token="2" delimiter=":" name="DeviceID" /><!-- Denotes CPU Instance eg: CPU1, CPU2..-->
			<Entity Id="CPU_Idle_Percentage" parseLine="2" delimiter=":" token="2" name="CPU_Idle_Percentage"/>			
			<Entity Id="LoadPercentage" parseLine="3" delimiter=":" token="2" name="LoadPercentage"/>					
			<Entity Id="CPU_Wait_Percentage" parseLine="4" delimiter=":" token="2" name="CPU_Wait_Percentage"/>
			<Entity Id="Name" parseLine="5" delimiter=":" token="2" name="Name"/><!-- Denotes Processor Name -->
		</Monitor>
		<Monitor Id="Top CPU Process" logOutput="false" script="true" command="script.sh" commandArgs="top_cpu" parseTag="top CPU process" interval="30" parseAll="true" parseImpl="TOP_CPU_PROCESS">
			<Entity Id="PID" token="1" delimiter="::" name="PID"/>
			<Entity Id="Name" token="2" delimiter="::" name="Name"/>
			<Entity Id="Path" token="3" delimiter="::" name="Path"/>
			<Entity Id="CPU" token="4" delimiter="::" name="CPU"/>
			<Entity Id="MEM" token="5" delimiter="::" name="MEM"/>
			<Entity Id="handle_count" token="6" delimiter="::" name="handle_count"/>
			<Entity Id="thread_count" token="7" delimiter="::" name="thread_count"/>
			<Entity Id="cmd_line_arg" token="8" delimiter="::" name="cmd_line_arg"/>
		</Monitor>
		<Monitor Id="Top MEM Process" logOutput="false" script="true" command="script.sh" commandArgs="top_mem" parseTag="top MEM process" interval="30" parseAll="true" parseImpl="TOP_MEM_PROCESS">
			<Entity Id="PID" token="1" delimiter="::" name="PID"/>
			<Entity Id="Name" token="2" delimiter="::" name="Name"/>
			<Entity Id="Path" token="3" delimiter="::" name="Path"/>
			<Entity Id="CPU" token="4" delimiter="::" name="CPU"/>
			<Entity Id="MEM" token="5" delimiter="::" name="MEM"/>
			<Entity Id="handle_count" token="6" delimiter="::" name="handle_count"/>
			<Entity Id="thread_count" token="7" delimiter="::" name="thread_count"/>
			<Entity Id="cmd_line_arg" token="8" delimiter="::" name="cmd_line_arg"/>
		</Monitor>
		<Monitor Id="CPU Cores Usage" script="true" command="script.sh" counter="true" commandArgs="core_usage" parseTag="cpu cores usage" interval="30" parseAll="true" parseImpl="CPU_IMPL">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="UserModeTime" token="2" delimiter="--" counter="true" expression="($UserModeTime-$UserModeTime_Previous)/$TimeDiff" name="UserModeTime" unique="Name"/>
			<Entity Id="NiceTime" token="3" delimiter="--" counter="true" expression="($NiceTime-$NiceTime_Previous)/$TimeDiff" name="NiceTime" unique="Name"/>
			<Entity Id="SystemModeTime" token="4" delimiter="--" counter="true" expression="($SystemModeTime-$SystemModeTime_Previous)/$TimeDiff" name="SystemModeTime" unique="Name"/>
			<Entity Id="IdleTime" token="5" delimiter="--" counter="true" expression="($IdleTime-$IdleTime_Previous)/$TimeDiff" name="IdleTime" unique="Name"/>
			<Entity Id="IOWaitTime" token="6" delimiter="--" counter="true" expression="($IOWaitTime-$IOWaitTime_Previous)/$TimeDiff" name="IOWaitTime" unique="Name"/>
			<Entity Id="InterruptServicingTime" token="7" delimiter="--" counter="true" expression="($InterruptServicingTime-$InterruptServicingTime_Previous)/$TimeDiff" name="InterruptServicingTime" unique="Name"/>
			<Entity Id="SoftirqsServicingTime" token="8" delimiter="--" counter="true" expression="($SoftirqsServicingTime-$SoftirqsServicingTime_Previous)/$TimeDiff" name="SoftirqsServicingTime" unique="Name"/>
			<Entity Id="OtherOsVirtualEnvTime" token="9" delimiter="--" counter="true" expression="($OtherOsVirtualEnvTime-$OtherOsVirtualEnvTime_Previous)/$TimeDiff" name="OtherOsVirtualEnvTime" unique="Name"/>
			<Entity Id="GuestOsVirtualCpuRunTime" token="10" delimiter="--" counter="true" expression="($GuestOsVirtualCpuRunTime-$GuestOsVirtualCpuRunTime_Previous)/$TimeDiff" name="GuestOsVirtualCpuRunTime" unique="Name"/>
			<Entity Id="NicedGuestRunTime" token="11" delimiter="--" counter="true" expression="($NicedGuestRunTime-$NicedGuestRunTime_Previous)/$TimeDiff" name="NicedGuestRunTime" unique="Name"/>
		</Monitor>
		<Monitor Id="RCA CPU Utilization" logOutput="false" alias="CPU details" script="true" command="script.sh" counter="true" commandArgs="rca_cpu_details" parseTag="rca cpu details" interval="30">
			<Entity Id="Name" parseLine="1" token="2" delimiter=":" name="Name" /><!-- Denotes CPU Instance eg: CPU1, CPU2..-->			
			<Entity Id="Load Percentage" parseLine="3" delimiter=":" token="2" name="Load Percentage"/>
			<Entity Id="ContextSwitchesPersec" parseLine="4" delimiter=":" token="2" name="Context Switches Per second" counter="true" expression="($ContextSwitchesPersec-$ContextSwitchesPersec_Previous)/$TimeDiff"/>
			<Entity Id="InterruptsPersec" parseLine="5" delimiter=":" token="2" name="Interrupts per second" counter="true" expression="($InterruptsPersec-$InterruptsPersec_Previous)/$TimeDiff"/>
		</Monitor>
		<Monitor Id="Disk Utilization" script="true" command="script.sh" commandArgs="disk_details" parseTag="disk details" interval="30" parseAll="true" parseImpl="LINUX_DISK_IMPL">
			<Entity Id="Name" token="1" delimiter="::" name="Name"/>
			<Entity Id="FileSystem" token="2" delimiter="::" name="FileSystem"/>
			<Entity Id="Type" token="3" delimiter="::" name="Type"/>
			<Entity Id="Size" token="4" delimiter="::" name="Size"/>
			<Entity Id="FreeSpace" token="5" delimiter="::" name="FreeSpace"/>
		</Monitor>
		<Monitor Id="Disk Inode" script="true" command="script.sh" commandArgs="disk_inode" parseTag="disk inode details" interval="30" parseAll="true" parseImpl="DISK_INODE_IMPL">
			<Entity Id="Name" token="1" delimiter="::" name="Name"/>
			<Entity Id="Total_Inodes" token="2" delimiter="::" name="Total_Inodes"/>
			<Entity Id="IUsed" token="3" delimiter="::" name="IUsed"/>
			<Entity Id="IFree" token="4" delimiter="::" name="IFree"/>
			<Entity Id="IUsedPer" token="5" delimiter="::" name="IUsedPer"/>
		</Monitor>
		<Monitor Id="RCA Disk Utilization" logOutput="false" alias="Disk details" script="true" command="script.sh" commandArgs="rca_disk_details" parseTag="rca disk details" interval="30" parseAll="true">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="Size" token="2" delimiter="--" name="Size"/>
			<Entity Id="Used" token="3" delimiter="--" name="Used"/>
			<Entity Id="Available" token="4" delimiter="--" name="Available"/>
			<Entity Id="Used Percentage" token="5" delimiter="--" name="Used Percentage"/>
		</Monitor>
		<Monitor Id="Memory Utilization" script="true" command="script.sh" commandArgs="mem_details" parseTag="memory details" interval="30" parseImpl="MEMORY_IMPL,ASSET_IMPL">
			<Entity Id="TotalVisibleMemorySize"  parseLine="1" token="2" delimiter=":" name="TotalVisibleMemorySize"/>
			<Entity Id="AvailablePhysicalMemory" parseLine="2" token="2" delimiter=":" name="AvailablePhysicalMemory"/>
			<Entity Id="FreePhysicalMemory" parseLine="3" token="2" delimiter=":" name="FreePhysicalMemory"/>
			<Entity Id="UsedPhysicalMemory" parseLine="4" token="2" delimiter=":" name="UsedPhysicalMemory"/>
			<Entity Id="BufferMemory" parseLine="5" token="2" delimiter=":" name="BufferMemory"/>
			<Entity Id="CacheMemory" parseLine="6" token="2" delimiter=":" name="CacheMemory"/>
			<Entity Id="TotalVirtualMemorySize" parseLine="7" token="2" delimiter=":" name="TotalVirtualMemorySize"/>
			<Entity Id="FreeVirtualMemory" parseLine="8" token="2" delimiter=":" name="FreeVirtualMemory"/>			
			<Entity Id="Caption" parseLine="9" token="2" delimiter=":" name="Caption"/><!-- Denotes OS Name -->			
		</Monitor>
		<Monitor Id="RCA Memory Utilization" logOutput="false" alias="Memory details" script="true" command="script.sh" commandArgs="rca_mem_details" parseTag="rca memory details" interval="30">
			<Entity Id="Total Memory" parseLine="1" token="2" delimiter=":" name="Total Memory"/>
			<Entity Id="Used Memory" parseLine="2" token="2" delimiter=":" name="Used Memory"/>
			<Entity Id="Free Memory" parseLine="3" token="2" delimiter=":" name="Free Memory"/>
			<Entity Id="Buffer Free Memory" parseLine="4" token="2" delimiter=":" name="Buffer Free Memory"/>			
			<Entity Id="Buffer Used Memory" parseLine="5" token="2" delimiter=":" name="Buffer Used Memory"/>
			<Entity Id="Total Virtual Memory" parseLine="6" token="2" delimiter=":" name="Total Virtual Memory"/>
			<Entity Id="Free Virtual Memory" parseLine="7" token="2" delimiter=":" name="Free Virtual Memory"/>			
		</Monitor>
		<Monitor Id="Number Of Cores" logOutput="false" type="static" script="true" command="script.sh" commandArgs="cpu_cores" parseTag="cpu cores" interval="30" parseImpl="ASSET_IMPL">
			<Entity Id="NumberOfCores" parseLine="1" token="2" delimiter=":" name="NumberOfCores"/>			
		</Monitor>
		<Monitor Id="OS Architecture" logOutput="false" type="static" script="true" command="script.sh" commandArgs="os_arch" parseTag="os architecture" interval="30" parseImpl="ASSET_IMPL">
			<Entity Id="OSArchitecture" parseLine="1" token="2" delimiter=":" name="OSArchitecture"/>			
		</Monitor>
		<Monitor Id="Adapter Details" script="true" command="script.sh" commandArgs="if_details" parseTag="interface details" parseAll="true" interval="30">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="NetConnectionStatus" token="2" delimiter="--" name="NetConnectionStatus"/>			
			<Entity Id="MACAddress" token="3" delimiter="--" name="MACAddress"/>			
		</Monitor>
		<Monitor Id="RCA Adapter Details" logOutput="false" alias="NIC Status" script="true" command="script.sh" commandArgs="rca_if_details" parseTag="rca interface details" parseAll="true" interval="30">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="NetConnectionStatus" token="2" delimiter="--" name="NetConnectionStatus"/>	
		</Monitor>
		<Monitor Id="Network Data" script="true" command="script.sh" counter="true" commandArgs="if_data" parseTag="interface data" interval="30" parseAll="true" parseImpl="NETWORK_IMPL">
			<Entity Id="AdapterDesc" token="1" delimiter="--" name="AdapterDesc"/>
			<Entity Id="Status" token="2" delimiter="--" name="Status"/>
			<Entity Id="MACAddress" token="3" delimiter="--" name="MACAddress"/>
			<Entity Id="BytesReceivedPersec" token="4" delimiter="--" name="BytesReceivedPersec" counter="true" expression="($BytesReceivedPersec-$BytesReceivedPersec_Previous)/$TimeDiff" unique="AdapterDesc"/>
			<Entity Id="PacketsReceivedUnicastPersec" token="5" delimiter="--" name="PacketsReceivedUnicastPersec" counter="true" expression="($PacketsReceivedUnicastPersec-$PacketsReceivedUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="PacketsReceivedNonUnicastPersec" default="0" token="10" delimiter="--" name="PacketsReceivedNonUnicastPersec" counter="true" expression="($PacketsReceivedNonUnicastPersec-$PacketsReceivedNonUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="BytesSentPersec" token="6" delimiter="--" name="BytesSentPersec" counter="true" expression="($BytesSentPersec-$BytesSentPersec_Previous)/$TimeDiff" unique="AdapterDesc"/>
			<Entity Id="PacketsSentUnicastPersec" token="7" delimiter="--" name="PacketsSentUnicastPersec" counter="true" expression="($PacketsSentUnicastPersec-$PacketsSentUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="PacketsSentNonUnicastPersec" default="0" token="15" name="PacketsSentNonUnicastPersec"/>
			<Entity Id="PacketsOutboundErrors" token="8" delimiter="--" name="PacketsOutboundErrors" counter="true" expression="$PacketsOutboundErrors-$PacketsOutboundErrors_Previous" unique="AdapterDesc"/>
			<Entity Id="PacketsOutboundDiscarded" token="9" delimiter="--" name="PacketsOutboundDiscarded" counter="true" expression="$PacketsOutboundDiscarded-$PacketsOutboundDiscarded_Previous" unique="AdapterDesc"/>
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>
			<!-- Bandwidth for interfaces are calculated in DataConsolidator.py > getNetworkData() -->
		</Monitor>
		<Monitor Id="Network Statistics" script="true" command="script.sh" counter="true" commandArgs="if_traffic" parseTag="interface traffic" interval="30" parseAll="true">
			<Entity Id="Name" token="1" name="Name"/>
			<Entity Id="BytesReceivedPersec" token="2" name="BytesReceivedPersec" counter="true" expression="($BytesReceivedPersec-$BytesReceivedPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsReceivedUnicastPersec" token="3" name="PacketsReceivedUnicastPersec" counter="true" expression="($PacketsReceivedUnicastPersec-$PacketsReceivedUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="PacketsReceivedNonUnicastPersec" default="0" token="15" name="PacketsReceivedNonUnicastPersec"/>
			<Entity Id="BytesSentPersec" token="4" name="BytesSentPersec" counter="true" expression="($BytesSentPersec-$BytesSentPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsSentUnicastPersec" token="5" name="PacketsSentUnicastPersec" counter="true" expression="($PacketsSentUnicastPersec-$PacketsSentUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="PacketsSentNonUnicastPersec" default="0" token="15" name="PacketsSentNonUnicastPersec"/>
			<Entity Id="PacketsOutboundErrors" token="6" name="PacketsOutboundErrors" counter="true" expression="$PacketsOutboundErrors-$PacketsOutboundErrors_Previous" unique="Name"/>
			<Entity Id="PacketsOutboundDiscarded" token="7" name="PacketsOutboundDiscarded" counter="true" expression="$PacketsOutboundDiscarded-$PacketsOutboundDiscarded_Previous" unique="Name"/>
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>
			<!-- Bandwidth for interfaces are calculated in DataConsolidator.py > getNetworkData() -->
		</Monitor>
		<Monitor Id="RCA Network Statistics" logOutput="false" alias="Network Statistics" script="true" command="script.sh" counter="true" commandArgs="rca_if_traffic" parseTag="rca interface traffic" interval="30" parseAll="true">
			<Entity Id="Name" token="1" name="Name"/>
			<Entity Id="BytesReceivedPersec" token="2" name="Rx Bytes Per second" counter="true" expression="($BytesReceivedPersec-$BytesReceivedPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsReceivedUnicastPersec" token="3" name="Packets Received Per second" counter="true" expression="($PacketsReceivedUnicastPersec-$PacketsReceivedUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="BytesSentPersec" token="4" name="Tx Bytes Per second" counter="true" expression="($BytesSentPersec-$BytesSentPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsSentUnicastPersec" token="5" name="Packets Sent Per second" counter="true" expression="($PacketsSentUnicastPersec-$PacketsSentUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="PacketsOutboundErrors" token="6" name="Packets Out bound Errors" counter="true" expression="$PacketsOutboundErrors-$PacketsOutboundErrors_Previous" unique="Name"/>
			<Entity Id="PacketsOutboundDiscarded" token="7" name="Packets Out bound Discarded" counter="true" expression="$PacketsOutboundDiscarded-$PacketsOutboundDiscarded_Previous" unique="Name"/>
		</Monitor>
		<Monitor Id="Number of Interrupts" logOutput="false" script="true" command="script.sh" counter="true" commandArgs="cpu_intr" parseTag="cpu interrupt" parseAll="true" interval="30" parseImpl="ROOT_IMPL">
			<Entity Id="InterruptsPersec" token="2" delimiter=":" name="InterruptsPersec" counter="true" expression="($InterruptsPersec-$InterruptsPersec_Previous)/$TimeDiff"/>
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>			
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>			
		</Monitor>
		<Monitor Id="Context Switches" logOutput="false" script="true" command="script.sh" counter="true" commandArgs="cpu_cs" parseTag="cpu context switches" parseAll="true" interval="30" parseImpl="ROOT_IMPL">
			<Entity Id="ContextSwitchesPersec" token="2" delimiter=":" name="ContextSwitchesPersec" counter="true" expression="($ContextSwitchesPersec-$ContextSwitchesPersec_Previous)/$TimeDiff"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>			
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>			
		</Monitor>
		<Monitor Id="Memory Statistics" script="true" command="script.sh" counter="true" commandArgs="mem_stats" parseTag="memory stats" interval="30" parseImpl="MEMORY_STATS_IMPL">
			<Entity Id="PagesInputPersec" parseLine="1" token="2" name="PagesInputPersec" counter="true" expression="($PagesInputPersec-$PagesInputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PagesOutputPersec" parseLine="2" token="2" name="PagesOutputPersec" counter="true" expression="($PagesOutputPersec-$PagesOutputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PageFaultsPersec" parseLine="3" token="2" name="PageFaultsPersec" counter="true" expression="($PageFaultsPersec-$PageFaultsPersec_Previous)/$TimeDiff"/>			
			<Entity Id="Timestamp_PerfTime" default="0" parseLine="15" token="15" name="Timestamp_PerfTime"/>
			<Entity Id="Frequency_PerfTime" default="0" parseLine="15" token="15" name="Frequency_PerfTime"/>			
		</Monitor>
		<Monitor Id="RCA Memory Statistics" logOutput="false" alias="Memory Statistics" script="true" command="script.sh" counter="true" commandArgs="mem_stats" parseTag="memory stats" interval="30">
			<Entity Id="PagesInputPersec" parseLine="1" token="2" name="PagesIn Per second" counter="true" expression="($PagesInputPersec-$PagesInputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PagesOutputPersec" parseLine="2" token="2" name="PagesOut Per second" counter="true" expression="($PagesOutputPersec-$PagesOutputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PageFaultsPersec" parseLine="3" token="2" name="PageFaults Per second" counter="true" expression="($PageFaultsPersec-$PageFaultsPersec_Previous)/$TimeDiff"/>		
		</Monitor>
		<Monitor Id="Disk Statistics" script="true" command="script.sh" counter="true" commandArgs="disk_stats" parseTag="disk stats" parseAll="true" interval="30" parseImpl="DISK_IO_IMPL">
			<Entity Id="Name" token="1" delimiter=":" name="Name"/>
			<Entity Id="DiskReadBytesPersec" token="2" delimiter=":" name="DiskReadBytesPersec" counter="true" expression="($DiskReadBytesPersec-$DiskReadBytesPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="DiskWriteBytesPersec" token="3" delimiter=":" name="DiskWriteBytesPersec" counter="true" expression="($DiskWriteBytesPersec-$DiskWriteBytesPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>			
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>			
		</Monitor>
		<Monitor Id="RCA Disk Statistics" logOutput="false" alias="Disk I/O" script="true" command="script.sh" counter="true" commandArgs="disk_stats" parseTag="disk stats" parseAll="true" interval="30">
			<Entity Id="Name" token="1" delimiter=":" name="Name"/>
			<Entity Id="DiskReadBytesPersec" token="2" delimiter=":" name="Read Bytes Per second" counter="true" expression="($DiskReadBytesPersec-$DiskReadBytesPersec_Previous)/$TimeDiff"/>
			<Entity Id="DiskWriteBytesPersec" token="3" delimiter=":" name="Write Bytes Per second" counter="true" expression="($DiskWriteBytesPersec-$DiskWriteBytesPersec_Previous)/$TimeDiff"/>	
		</Monitor>
		<Monitor Id="PROCESS_AND_SERVICE_DETAILS" script="true" command="script.sh" saveToFile="true" logOutput="false" commandArgs="topCommand,psCommand" parseTag="top,ps" bufferSize="1" interval="30" parseImpl="PROCESS_IMPL"/>
		<Monitor Id="Process_Monitoring" script="true" command="script.sh" saveToFile="true" logOutput="false" commandArgs="ProcessesCommand" parseTag="process_monitoring" bufferSize="1" interval="30" parseImpl="PROCESS_IMPLEMENTATION"/>
		<Monitor Id="Disk Errors" logOutput="false" script="true" command="script.sh" commandArgs="disk_err" parseTag="disk errors" parse="false" bufferSize="1" interval="30"/>
		<Monitor Id="Dmesg Errors" logOutput="false" script="true" command="script.sh" commandArgs="dmesg_err" parse="false" parseTag="dmesg errors" bufferSize="1" interval="30"/>
		<Monitor Id="Uptime Details" logOutput="false" alias="Uptime" script="true" command="script.sh" commandArgs="uptime_details" parse="false" parseTag="uptime details" bufferSize="1" interval="30"/>	
		<Monitor Id="Installed Software" script="true" command="software.sh" logOutput="false" commandArgs="inst_soft" parseAll="true" parseTag="installed software" bufferSize="1" interval="30">
			<Entity Id="Software Name" token="1" delimiter=":::" name="Name"/>
			<Entity Id="Version" token="2" delimiter=":::" name="Version"/>
			<Entity Id="Description" token="3" delimiter=":::" name="Description"/>
		</Monitor>
		<Monitor Id="User Sessions" logOutput="false" script="true" command="script.sh" commandArgs="user_sessions" parseTag="user sessions" parseAll="true" bufferSize="1" interval="30">
			<Entity Id="User Name" token="1" delimiter=":::" name="User Name"/>
			<Entity Id="Terminal" token="2" delimiter=":::" name="Terminal"/>
			<Entity Id="Host Name" token="3" delimiter=":::" name="Host Name"/>
			<Entity Id="Session Period" token="4" delimiter=":::" name="Session Period"/>
		</Monitor>
		<Monitor Id="System Stats" logOutput="true" script="true" command="script.sh" commandArgs="system_stats" parseTag="system_stats" parseAll="true" bufferSize="1" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="1 min" token="1" delimiter=" " name="Last 1 min Avg"/>
			<Entity Id="5 min" token="2" delimiter=" " name="Last 5 min Avg"/>
			<Entity Id="15 min" token="3" delimiter=" " name="Last 15 min Avg"/>
			<Entity Id="Process Count" token="4" delimiter=" " name="Process Count"/>
			<Entity Id="TotalProcessCount" token="6" delimiter=" " name="TotalProcessCount"/>
			<Entity Id="ListeningSocketCount" token="7" delimiter=" " name="ListeningSocketCount"/>
		</Monitor>
		<Monitor Id="System Uptime" logOutput="true" script="true" command="script.sh" counter="true" commandArgs="system_uptime" parseAll="true" parseTag="system_uptime" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="Utime" token="1" delimiter=" " counter="true" expression="($Utime-$Utime_Previous)" name="Utime"/>
			<Entity Id="IdleTime" token="2" delimiter=" " counter="true" expression="($IdleTime-$IdleTime_Previous)" name="IdleTime"/>
		</Monitor>
		<Monitor Id="Process Queue" logOutput="true" script="true" command="script.sh" commandArgs="process_queue" parseTag="process_queue" bufferSize="1" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="Procs Running" parseLine="1" token="2" delimiter=" " name="Procs Running"/>
			<Entity Id="Procs Blocked" parseLine="2" token="2" delimiter=" " name="Procs Blocked"/>			
		</Monitor>
		<Monitor Id="Login Count" logOutput="false" script="true" command="script.sh" commandArgs="login_count" parseTag="login_count" bufferSize="1" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="Login Count" parseLine="2" token="2" delimiter="=" name="Login Count"/>			
		</Monitor>
		<Monitor Id="PSUtilMonitoring" logOutput="false" script="true" command="script.sh" commandArgs="disk_err" parseTag="disk errors" parse="false" bufferSize="1" interval="30" parseImpl="PSUTIL_IMPL"/>
		<Monitor Id="CPU_Monitoring" logOutput="true" script="true" command="script.sh" commandArgs="cpu_stats" parseTag="cpu_stats" parse="false" bufferSize="1" interval="30" parseImpl="CPU_STATS_IMPL" parseAll="true"/>
	</LinuxMonitors>
</MonitorsXml>
