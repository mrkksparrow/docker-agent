<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<!-- DO NOT EDIT THIS FILE. CUSTOM CHANGES MUST BE DONE IN /opt/site24x7/monagent/conf/custom_monitors.xml-->
<!-- 
Attributes :
	script		=	"script=true" tells the agent to append script location before the "command=cpu.sh" attribute
	sendValue	=	"sendValue=false" implies attribute value parsed from command output will not be sent to server
 -->
<MonitorsXml>
	<OSXMonitors>
	<Monitor Id="Disk Utilization" script="true" command="osx_script.sh" commandArgs="disk_details" parseTag="disk details" interval="30" parseAll="true" parseImpl="DISK_IMPL">
			<Entity Id="Type" token="1" delimiter="--" name="Type"/>
			<Entity Id="FileSystem" token="2" delimiter="--" name="FileSystem"/>
			<Entity Id="Name" token="3" delimiter="--" name="Name"/>
			<Entity Id="Total_Inodes" token="4" delimiter="--" name="Total_Inodes"/>
			<Entity Id="IUsed" token="5" delimiter="--" name="IUsed"/>
			<Entity Id="IFree" token="6" delimiter="--" name="IFree"/>
			<Entity Id="IUsedPer" token="7" delimiter="--" name="IUsedPer"/>
			<Entity Id="Size" token="8" delimiter="--" name="Size"/>
			<Entity Id="FreeSpace" token="9" delimiter="--" name="FreeSpace"/>
	</Monitor>
	<Monitor Id="RCA Disk Utilization" alias="Disk details" script="true" command="osx_script.sh" commandArgs="rca_disk_details" parseTag="rca disk details" interval="30" parseAll="true">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="Size" token="2" delimiter="--" name="Size"/>
			<Entity Id="Used" token="3" delimiter="--" name="Used"/>
			<Entity Id="Available" token="4" delimiter="--" name="Available"/>
			<Entity Id="Used Percentage" token="5" delimiter="--" name="Used Percentage"/>
	</Monitor>
	<Monitor Id="Memory Utilization" script="true" command="osx_script.sh" commandArgs="mem_details" parseTag="memory details" interval="30" parseImpl="MEMORY_IMPL,ASSET_IMPL">
			<Entity Id="TotalVisibleMemorySize"  parseLine="1" token="2" delimiter=":" name="TotalVisibleMemorySize"/>
			<Entity Id="FreePhysicalMemory" parseLine="2" token="2" delimiter=":" name="FreePhysicalMemory"/>
			<Entity Id="UsedPhysicalMemory" parseLine="3" token="2" delimiter=":" name="UsedPhysicalMemory"/>
			<Entity Id="TotalVirtualMemorySize" parseLine="4" token="2" delimiter=":" name="TotalVirtualMemorySize"/>
			<Entity Id="FreeVirtualMemory" parseLine="5" token="2" delimiter=":" name="FreeVirtualMemory"/>			
			<Entity Id="Caption" parseLine="6" token="2" delimiter=":" name="Caption"/><!-- Denotes OS Name -->			
	</Monitor>
	<Monitor Id="RCA Memory Utilization" alias="Memory details" script="true" command="osx_script.sh" commandArgs="mem_details" parseTag="memory details" interval="30">
			<Entity Id="Total Memory" parseLine="1" token="2" delimiter=":" name="Total Memory"/>
			<!--  <Entity Id="Used Memory" parseLine="3" token="2" delimiter=":" name="Used Memory"/> -->
			<Entity Id="Free Memory" parseLine="2" token="2" delimiter=":" name="Free Memory"/>
			<!--  <Entity Id="Buffer Free Memory" parseLine="4" token="2" delimiter=":" name="Buffer Free Memory"/>			
			<Entity Id="Buffer Used Memory" parseLine="5" token="2" delimiter=":" name="Buffer Used Memory"/> -->
			<Entity Id="Total Virtual Memory" parseLine="4" token="2" delimiter=":" name="Total Virtual Memory"/>
			<Entity Id="Free Virtual Memory" parseLine="5" token="2" delimiter=":" name="Free Virtual Memory"/>			
	</Monitor>
	<Monitor Id="OS Architecture" type="static" script="true" command="osx_script.sh" commandArgs="os_arch" parseTag="os architecture" interval="30" parseImpl="ASSET_IMPL">
			<Entity Id="OSArchitecture" parseLine="1" token="2" delimiter=":" name="OSArchitecture"/>			
	</Monitor>
	<Monitor Id="Number of Interrupts" script="true" command="osx_script.sh" counter="true" commandArgs="cpu_intr" parseTag="cpu interrupt" parseAll="true" interval="30" parseImpl="ROOT_IMPL">
			<Entity Id="InterruptsPersec" token="2" delimiter=":" name="InterruptsPersec" counter="true" expression="($InterruptsPersec-$InterruptsPersec_Previous)/$TimeDiff"/>
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>			
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>			
	</Monitor>
	<Monitor Id="Context Switches" script="true" command="osx_script.sh" counter="true" commandArgs="cpu_cs" parseTag="cpu context switches" parseAll="true" interval="30" parseImpl="ROOT_IMPL">
			<Entity Id="ContextSwitchesPersec" token="2" delimiter=":" name="ContextSwitchesPersec" counter="true" expression="($ContextSwitchesPersec-$ContextSwitchesPersec_Previous)/$TimeDiff"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>			
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>	
	</Monitor>
	<Monitor Id="Memory Statistics" script="true" command="osx_script.sh" counter="true" commandArgs="mem_stats" parseTag="memory stats" interval="30" parseImpl="MEMORY_STATS_IMPL">
			<Entity Id="PagesInputPersec" parseLine="1" token="2" name="PagesInputPersec" counter="true" expression="($PagesInputPersec-$PagesInputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PagesOutputPersec" parseLine="2" token="2" name="PagesOutputPersec" counter="true" expression="($PagesOutputPersec-$PagesOutputPersec_Previous)/$TimeDiff"/>
			<Entity Id="PageFaultsPersec" parseLine="3" token="2" name="PageFaultsPersec" counter="true" expression="($PageFaultsPersec-$PageFaultsPersec_Previous)/$TimeDiff"/>			
			<Entity Id="Timestamp_PerfTime" default="0" parseLine="15" token="15" name="Timestamp_PerfTime"/>
			<Entity Id="Frequency_PerfTime" default="0" parseLine="15" token="15" name="Frequency_PerfTime"/>			
	</Monitor>
	<Monitor Id="CPU Utilization" script="true" command="osx_script.sh" commandArgs="cpu_util,processor" parseTag="cpu details,processor" interval="30" parseImpl="ROOT_IMPL,ASSET_IMPL">
			<Entity Id="DeviceID" parseLine="1" token="2" delimiter=":" name="DeviceID" /><!-- Denotes CPU Instance eg: CPU1, CPU2..-->
			<Entity Id="CPU_Idle_Percentage" parseLine="2" delimiter=":" token="2" sendValue="false" name="CPU_Idle_Percentage"/>			
			<Entity Id="LoadPercentage" parseLine="3" delimiter=":" token="2" name="LoadPercentage"/>					
			<Entity Id="Name" parseLine="4" delimiter=":" token="2" name="Name"/><!-- Denotes Processor Name -->
	</Monitor>
	<Monitor Id="RCA CPU Utilization" alias="CPU details" script="true" command="osx_script.sh" counter="true" commandArgs="rca_cpu_details" parseTag="rca cpu details" interval="30">
			<Entity Id="Name" parseLine="1" token="2" delimiter=":" name="Name" /><!-- Denotes CPU Instance eg: CPU1, CPU2..-->			
			<Entity Id="Load Percentage" parseLine="3" delimiter=":" token="2" name="Load Percentage"/>
			<Entity Id="ContextSwitchesPersec" parseLine="4" delimiter=":" token="2" name="Context Switches Per second" counter="true" expression="($ContextSwitchesPersec-$ContextSwitchesPersec_Previous)/$TimeDiff"/>
			<Entity Id="InterruptsPersec" parseLine="5" delimiter=":" token="2" name="Interrupts per second" counter="true" expression="($InterruptsPersec-$InterruptsPersec_Previous)/$TimeDiff"/>
	</Monitor>
	<Monitor Id="Number Of Cores" type="static" script="true" command="osx_script.sh" commandArgs="cpu_cores" parseTag="cpu cores" interval="30" parseImpl="ASSET_IMPL">
			<Entity Id="NumberOfCores" parseLine="1" token="2" delimiter=":" name="NumberOfCores"/>			
	</Monitor>
	<Monitor Id="Disk Statistics" script="true" command="osx_script.sh" counter="true" commandArgs="disk_stats" parseTag="disk stats" parseAll="true" interval="30" parseImpl="SOURCE_DISK_IO_IMPL">
			<Entity Id="Name" token="1" delimiter=":" name="Name"/>
			<Entity Id="DiskReadBytesPersec" token="2" delimiter=":" name="DiskReadBytesPersec" counter="true" expression="($DiskReadBytesPersec-$DiskReadBytesPersec_Previous)/$TimeDiff"/>
			<Entity Id="DiskWriteBytesPersec" token="3" delimiter=":" name="DiskWriteBytesPersec" counter="true" expression="($DiskWriteBytesPersec-$DiskWriteBytesPersec_Previous)/$TimeDiff"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>		
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>
	</Monitor>
	<Monitor Id="RCA Disk Statistics" alias="Disk I/O" script="true" command="osx_script.sh" counter="true" commandArgs="disk_stats" parseTag="disk stats" parseAll="true" interval="30">
			<Entity Id="Name" token="1" delimiter=":" name="Name"/>
			<Entity Id="DiskReadBytesPersec" token="2" delimiter=":" name="Read Bytes Per second" counter="true" expression="($DiskReadBytesPersec-$DiskReadBytesPersec_Previous)/$TimeDiff"/>
			<Entity Id="DiskWriteBytesPersec" token="3" delimiter=":" name="Write Bytes Per second" counter="true" expression="($DiskWriteBytesPersec-$DiskWriteBytesPersec_Previous)/$TimeDiff"/>	
	</Monitor>
	<Monitor Id="RCA Adapter Details" alias="NIC Status" script="true" command="osx_script.sh" commandArgs="rca_if_details" parseTag="rca interface details" parseAll="true" interval="30">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="NetConnectionStatus" token="2" delimiter="--" name="NetConnectionStatus"/>	
	</Monitor>
	<Monitor Id="RCA Network Statistics" alias="Network Statistics" script="true" command="osx_script.sh" counter="true" commandArgs="rca_if_traffic" parseTag="rca interface traffic" interval="30" parseAll="true">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="BytesReceivedPersec" token="2" delimiter="--" name="Rx Bytes Per second" counter="true" expression="($BytesReceivedPersec-$BytesReceivedPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsReceivedUnicastPersec" token="3" delimiter="--" name="Packets Received Per second" counter="true" expression="($PacketsReceivedUnicastPersec-$PacketsReceivedUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="BytesSentPersec" token="4" delimiter="--" name="Tx Bytes Per second" counter="true" expression="($BytesSentPersec-$BytesSentPersec_Previous)/$TimeDiff" unique="Name"/>
			<Entity Id="PacketsSentUnicastPersec" token="5" delimiter="--" name="Packets Sent Per second" counter="true" expression="($PacketsSentUnicastPersec-$PacketsSentUnicastPersec_Previous)" unique="Name"/>
			<Entity Id="PacketsOutboundErrors" default="0" token="7" delimiter="--" name="Packets Out bound Errors" counter="true" expression="$PacketsOutboundErrors-$PacketsOutboundErrors_Previous" unique="Name"/>
			<Entity Id="PacketsOutboundDiscarded" token="6" delimiter="--" name="Packets Out bound Discarded" counter="true" expression="$PacketsOutboundDiscarded-$PacketsOutboundDiscarded_Previous" unique="Name"/>
	</Monitor>
	<Monitor Id="Network Data" script="true" command="osx_script.sh" counter="true" commandArgs="if_data" parseTag="interface data" interval="30" parseAll="true" parseImpl="NETWORK_IMPL">
			<Entity Id="AdapterDesc" token="1" delimiter="--" name="AdapterDesc"/>
			<Entity Id="Status" token="2" delimiter="--" name="Status"/>
			<Entity Id="MACAddress" token="3" delimiter="--" name="MACAddress"/>
			<Entity Id="BytesReceivedPersec" token="5" delimiter="--" name="BytesReceivedPersec" counter="true" expression="($BytesReceivedPersec-$BytesReceivedPersec_Previous)/$TimeDiff" unique="AdapterDesc"/>
			<Entity Id="PacketsReceivedUnicastPersec" token="4" delimiter="--" name="PacketsReceivedUnicastPersec" counter="true" expression="($PacketsReceivedUnicastPersec-$PacketsReceivedUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="PacketsReceivedNonUnicastPersec" default="0" token="15" delimiter="--" name="PacketsReceivedNonUnicastPersec" counter="true" expression="($PacketsReceivedNonUnicastPersec-$PacketsReceivedNonUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="BytesSentPersec" token="7" delimiter="--" name="BytesSentPersec" counter="true" expression="($BytesSentPersec-$BytesSentPersec_Previous)/$TimeDiff" unique="AdapterDesc"/>
			<Entity Id="PacketsSentUnicastPersec" token="6" delimiter="--" name="PacketsSentUnicastPersec" counter="true" expression="($PacketsSentUnicastPersec-$PacketsSentUnicastPersec_Previous)" unique="AdapterDesc"/>
			<Entity Id="PacketsSentNonUnicastPersec" default="0" token="15" name="PacketsSentNonUnicastPersec"/>
			<Entity Id="PacketsOutboundErrors" default="0" token="9" delimiter="--" name="PacketsOutboundErrors" counter="true" expression="$PacketsOutboundErrors-$PacketsOutboundErrors_Previous" unique="AdapterDesc"/>
			<Entity Id="PacketsOutboundDiscarded" token="8" delimiter="--" name="PacketsOutboundDiscarded" counter="true" expression="$PacketsOutboundDiscarded-$PacketsOutboundDiscarded_Previous" unique="AdapterDesc"/>
			<Entity Id="Timestamp_PerfTime" default="0" token="15" name="Timestamp_PerfTime"/>
			<Entity Id="CurrentBandwidth" default="100000000" token="30" name="CurrentBandwidth"/>
			<Entity Id="Frequency_PerfTime" default="0" token="15" name="Frequency_PerfTime"/>
			<Entity Id="Ipv4Addrs" token="10" delimiter="--" name="Ipv4Addrs" />
            <Entity Id="Ipv6Addrs" token="11" delimiter="--" name="Ipv6Addrs" />
	</Monitor>
	<Monitor Id="System Stats" script="true" command="osx_script.sh" commandArgs="load_data" parseTag="CPU Load" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="1 min" parseLine="1" delimiter="--" token="2" name="Last 1 min Avg" />
			<Entity Id="5 min" parseLine="2" delimiter="--" token="2" name="Last 5 min Avg"/>			
			<Entity Id="15 min" parseLine="3" delimiter="--" token="2" name="Last 15 min Avg"/>
			<Entity Id="Process Count" parseLine="4" delimiter="--" token="2" name="Process Count"/>
			<Entity Id="ListeningSocketCount" parseLine="5" token="2" delimiter="--" name="ListeningSocketCount"/>
	</Monitor>
	<Monitor Id="PROCESS_AND_SERVICE_DETAILS" script="true" command="osx_script.sh" saveToFile="true" logOutput="false" commandArgs="topCommand,psCommand" parseTag="top,ps" bufferSize="1" interval="30" parseImpl="PROCESS_IMPL"/>
	<Monitor Id="CPU Cores Usage" script="true" command="osx_script.sh" counter="true" commandArgs="core_usage" parseTag="cpu cores usage" interval="30" parseAll="true" parseImpl="CPU_IMPL">
			<Entity Id="Name" token="1" delimiter="--" name="Name"/>
			<Entity Id="UserModeTime" default="0" token="2" delimiter="--" counter="true" expression="($UserModeTime-$UserModeTime_Previous)/$TimeDiff" name="UserModeTime" unique="Name"/>
			<Entity Id="NiceTime" default="0" token="3" delimiter="--" counter="true" expression="($NiceTime-$NiceTime_Previous)/$TimeDiff" name="NiceTime" unique="Name"/>
			<Entity Id="SystemModeTime" default="0" token="4" delimiter="--" counter="true" expression="($SystemModeTime-$SystemModeTime_Previous)/$TimeDiff" name="SystemModeTime" unique="Name"/>
			<Entity Id="IdleTime" default="0" token="5" delimiter="--" counter="true" expression="($IdleTime-$IdleTime_Previous)/$TimeDiff" name="IdleTime" unique="Name"/>
			<Entity Id="IOWaitTime" default="0" token="6" delimiter="--" counter="true" expression="($IOWaitTime-$IOWaitTime_Previous)/$TimeDiff" name="IOWaitTime" unique="Name"/>
			<Entity Id="InterruptServicingTime" default="0" token="7" delimiter="--" counter="true" expression="($InterruptServicingTime-$InterruptServicingTime_Previous)/$TimeDiff" name="InterruptServicingTime" unique="Name"/>
			<Entity Id="SoftirqsServicingTime" default="0" token="8" delimiter="--" counter="true" expression="($SoftirqsServicingTime-$SoftirqsServicingTime_Previous)/$TimeDiff" name="SoftirqsServicingTime" unique="Name"/>
			<Entity Id="OtherOsVirtualEnvTime" default="0" token="9" delimiter="--" counter="true" expression="($OtherOsVirtualEnvTime-$OtherOsVirtualEnvTime_Previous)/$TimeDiff" name="OtherOsVirtualEnvTime" unique="Name"/>
			<Entity Id="GuestOsVirtualCpuRunTime" default="0" token="10" delimiter="--" counter="true" expression="($GuestOsVirtualCpuRunTime-$GuestOsVirtualCpuRunTime_Previous)/$TimeDiff" name="GuestOsVirtualCpuRunTime" unique="Name"/>
			<Entity Id="NicedGuestRunTime" default="0" token="11" delimiter="--" counter="true" expression="($NicedGuestRunTime-$NicedGuestRunTime_Previous)/$TimeDiff" name="NicedGuestRunTime" unique="Name"/>
			<Entity Id="PercentProcessorTime" token="2" delimiter="--" name="PercentProcessorTime"/>
	</Monitor>
	<Monitor Id="User Sessions" script="true" command="osx_script.sh" logOutput="false" commandArgs="user_sessions" parseTag="user sessions" parseAll="true" bufferSize="1" interval="30">
			<Entity Id="User Name" token="1" delimiter=":::" name="User Name"/>
			<Entity Id="Terminal" token="2" delimiter=":::" name="Terminal"/>
			<Entity Id="Host Name" token="3" delimiter=":::" name="Host Name"/>
			<Entity Id="Session Period" token="4" delimiter=":::" name="Session Period"/>
	</Monitor>
	<Monitor Id="Disk Errors" script="true" command="osx_script.sh" commandArgs="disk_err" parseTag="disk errors" parse="false" bufferSize="1" interval="30"/>
	<Monitor Id="Dmesg Errors" script="true" command="osx_script.sh" commandArgs="dmesg_err" parse="false" parseTag="dmesg errors" bufferSize="1" interval="30"/>
	<Monitor Id="Uptime Details" alias="Uptime" script="true" command="osx_script.sh" commandArgs="uptime_details" parse="false" parseTag="uptime details" bufferSize="1" interval="30"/>
	<Monitor Id="Login Count" script="true" command="osx_script.sh" logOutput="false" commandArgs="login_count" parseTag="login_count" bufferSize="1" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="Login Count" parseLine="2" token="2" delimiter="=" name="Login Count"/>			
	</Monitor>
	<Monitor Id="Process Queue" script="true" command="osx_script.sh" logOutput="false" commandArgs="process_queue" parseTag="process_queue" bufferSize="1" interval="30" parseImpl="SYSTEM_IMPL">
			<Entity Id="Procs Running" parseLine="1" token="2" delimiter=" " name="Procs Running"/>
			<Entity Id="Procs Blocked" parseLine="2" token="2" delimiter=" " name="Procs Blocked"/>			
	</Monitor>
	</OSXMonitors>
</MonitorsXml>
